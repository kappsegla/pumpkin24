<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pumpkin.HelloController">
    <Label fx:id="textLabel" />
    <TextField fx:id="textField" onKeyReleased="#textFieldChanged" />
    <Button fx:id="transferButton" onAction="#transferButtonAction" text="Transfer text" disable="true" />
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
